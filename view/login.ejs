
<% var title = 'Authentication Example' %>
<% include head %>

<h1>Login</h1>
<%- message %>
<form method="post" action="/login">
  <p>
    <label>Email:</label>
    <input type="text" name="email">
  </p>
  <p>
    <label>Password:</label>
    <input type="password" name="password">
  </p>
  <p>
    <input type="submit" value="Login">
  </p>
</form>

<a href="/signup">Sign up</a>

<% include foot %>
