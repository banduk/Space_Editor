
<% var title = 'Signup' %>
<% include head %>

<h1>Signup</h1>
<%- message %>
<form method="post" action="/signup">
  <p>
    <label>Username:</label>
    <input type="text" name="username">
  </p>
  <p>
    <label>Password:</label>
    <input type="text" name="password">
  </p>
  <p>
    <input type="submit" value="Signup">
  </p>
</form>

<% include foot %>
